<%= f.label "Address Type" %>
<%= f.input :ship_type, :collection => Address::SHIPTYPE, :include_blank => false, label: false %>
<%= f.label "Address 1" %>
<%= f.text_field :line_one %>
<%= f.label "Address 2" %>
<%= f.text_field :line_two %>
<%= f.label "City" %>
<%= f.text_field :city %>
<%= f.label "State" %>
<%= f.select :state, :collection => Address::STATE %>
<%= f.label "Zip Code" %>
<%= f.text_field :zip %>
<%= f.hidden_field :customer_id %>