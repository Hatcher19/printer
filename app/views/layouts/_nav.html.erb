<div class="row-fluid navigation">
	<div class="span12">
		<div class="brand pull-left">
			<% if user_signed_in? %>
				Print On Rails
			<% else %>
				Print On Rails
			<% end %>
		</div>
		<% if user_signed_in? %>
			<div>
				<%= link_to "Orders", orders_path %>
				<%= link_to "Users", users_path %>
				<%= link_to "Accounts", accounts_path %>
				<%= link_to "New User", new_user_registration_path %>
			</div>
		<% end %>
		<div class="pull-right">
			<% if user_signed_in? %>
				<%= current_user.email %>
				<%= link_to "Logout", destroy_user_session_path, method: :delete %>
			<% else %>
				<%= link_to "Login", new_user_session_path, class: "btn btn-default btn-lg" %>
				<%= link_to "Sign Up", new_account_path, class: "btn btn-primary btn-lg" %>
			<% end %>
		</div>
	</div>
</div>