<div class="row">
  <div class="span5">
  </div>
  <div class="box center span3">
    <div class="pricing-header">
      <h3>Basic</h3>
    </div>
    <div class="money-per-month">
      <h1>$50 /mo</h1>
      <%= simple_form_for @subscription, url: url, html: { class: 'form-horizontal' } do |f| %>
        <div class="divider"></div>
        <ul class="text-decoration-none">
          <li></li>
        </ul>
        <%= f.radio_button :plan_id, "basic", id: 'basic', :checked => true %><br><br>
        <%= f.button :submit, "Finished", class: 'btn btn-danger btn-large' %>
      <% end %>
    </div>
  </div>
</div>